# Directory structure
dir /dev 755 0 0

# Device nodes
nod /dev/console 644 0 0 c 5 1
nod /dev/loop0 644 0 0 b 7 0
nod /dev/ttyS0 644 0 0 b 4 64
nod /dev/mem 644 0 0 c 1 1

# Upload the two frames that fish will display
file /frame0.txt lab0-fish/frame0.txt 755 0 0
file /frame1.txt lab0-fish/frame1.txt 755 0 0

# Run the userspace test code for fish
file /init .lab0-fish/kernel_init 755 0 0
